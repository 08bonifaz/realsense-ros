<launch>

    <include file="$(find realsense_ros_person)/launch/cam_track.launch"/>

    <node pkg="nodelet" type="nodelet" name="realsense_ros_person_sample_nodelet"  args="manager" output="screen"/>
        <node pkg="realsense_ros_person" type="realsense_ros_person_sample" name="realsense_ros_person_sample"
            args="load realsense_ros_person/realsense_ros_person_sample node" output="screen">
            <param name="recognition"      type="bool"  value="true" />
            <param name="skeleton"         type="bool"  value="true" />
            <param name="gestures"         type="bool"  value="true" />
            <param name="landmarks"        type="bool"  value="true" />
            <param name="headBoundingBox"  type="bool"  value="true" />
            <param name="headPose"         type="bool"  value="true" />
        </node>

</launch> 
